{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["search"],
  "permissions": [
    "core:app:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "core:window:allow-start-dragging",
    "core:window:allow-set-size"
  ]
}
